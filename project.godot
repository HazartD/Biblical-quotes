; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Biblical quotes"
run/main_scene="res://scene/Main_Menu.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="HazartD/Biblical"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Global="*res://script/global.gd"

[display]

window/size/viewport_width=500
window/size/viewport_height=500

[internationalization]

locale/translations=PackedStringArray("res://traduction/Books.en.translation", "res://traduction/Books.es.translation", "res://traduction/ui.en.translation", "res://traduction/ui.es.translation")

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
